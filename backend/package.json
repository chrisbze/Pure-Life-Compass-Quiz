{
  "name": "pure-life-compass-quiz-api",
  "version": "1.0.0",
  "description": "Always Worksâ„¢ Pure Life Compass Quiz - Backend API with GHL Integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --verbose --detectOpenHandles",
    "test:integration": "jest tests/integration --verbose",
    "test:ghl": "jest tests/ghl --verbose", 
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "health-check": "curl -f http://localhost:3000/api/health-check || exit 1"
  },
  "keywords": [
    "quiz",
    "lead-generation", 
    "gohighlevel",
    "ghl",
    "webhook",
    "always-works"
  ],
  "author": "Pure Life Warrior",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "uuid": "^9.0.1",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "*.js",
      "!node_modules/**",
      "!coverage/**"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "verbose": true,
    "forceExit": true,
    "detectOpenHandles": true
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}